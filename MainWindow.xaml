<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="47*"/>
            <RowDefinition Height="387*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="MainWindow_Frame" Content="" Grid.RowSpan="2" Margin="0,47,0,0"/>
        <StackPanel Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Center" Width="780" Height="37" VerticalAlignment="Top">
            <Button Content="Главная страница" Height="30" Click="Button_Click"/>
            <Button Content="Список сотрудников" Height="30" Click="Button_Click_1"/>
        </StackPanel>

    </Grid>
</Window>
